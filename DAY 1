'''def fact(n):
    if n==1:
        return 1
    else:
        return n*fact(n-1)
print(fact(1))'''

'''def sum(a,b):
    return(a+b)
c=sum(5,4)
print(c)'''

'''def pacakage(*a):
    print(a)
pacakage(4,2,3)'''

'''def package(**a):
    print(a)
package(i=1,h=3)'''

'''def hi(a,b,c):
    print(a,b,c)
hi(*(1,2,3))'''

'''def hi(a,b,c):
    print(a,b,c)
hi(*{'a':2,'b':3,'c':4})'''

'''#keyword argument
def hi(*a,**b):
    print(a)
    print(b)
hi(1,2,3,a=2,b=3)'''
