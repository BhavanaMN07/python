'''#separate the characters in index
a="1EW22CS4@#"
num=""
alpa=""
spl=""
i=0
while i<len(a):
    if '0'<=a[i]<='9':
        num=num+a[i]
    elif 'a'<=a[i]<='z' or 'A'<=a[i]<='Z':
        alpa=alpa+a[i]
    else:
        spl=spl+a[i]
    i=i+1
print(num)
print(alpa)
print(spl)'''

'''a = "@#1EW21CS026!$%"
sp = ""
i= 0
while i< len(a):
    if not ('0'<=a[i]<='9' or 'a'<=a[i]<='z' or 'A'<=a[i]<='Z'):
        sp+=a[i]
    i += 1
print("Special Characters:", sp)'''

